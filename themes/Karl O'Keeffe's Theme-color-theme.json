{
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#686868"
      }
    },
    {
      "name": "Unused code",
      "scope": ["comment.unused"],
      "settings": {
        "fontStyle": "",
        "foreground": "#686868"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#c2ad00"
      }
    },
    {
      "name": "String punctuation",
      "scope": ["punctuation.definition.string.begin", "punctuation.definition.string.end"],
      "settings": {
        "foreground": "#868686"
      }
    },
    {
      "name": "Symbol",
      "scope": ["constant.language.symbol"],
      "settings": {
        "foreground": "#8fa738"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#993c26"
      }
    },
    {
      "name": "Number period",
      "scope": "meta.delimiter.decimal.period",
      "settings": {
        "foreground": "#6c3122"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#2bba8a"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other",
        "variable.other.constant.property"
      ],
      "settings": {
        "foreground": "#b34d1b"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#b2987f"
      }
    },
    {
      "name": "Variable definition / Parameter",
      "scope": ["meta.definition variable", "variable.parameter"],
      "settings": {
        "foreground": "#c6975c"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Language variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#797979"
      }
    },
    {
      "name": "This",
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": {
        "foreground": "#8b84bc"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#577aa0"
      }
    },
    {
      "name": "Function",
      "scope": [
        "meta.definition entity.name.function",
        "meta.object-literal.key entity.name.function",
        "meta.function.method entity.name.function"
      ],
      "settings": {
        "foreground": "#397cb3"
      }
    },
    {
      "name": "Function parameter brackets",
      "scope": [
        "punctuation.definition.parameters",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#697b90"
      }
    },
    {
      "name": "Template expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#afafaf"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#797979"
      }
    },
    {
      "name": "Keyword control flow",
      "scope": [
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.trycatch",
        "keyword.control.exception",
      ],
      "settings": {
        "foreground": "#cabf82"
      }
    },
    {
      "name": "Generator and async",
      "scope": [
        "keyword.generator.asterisk",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#a89d6c"
      }
    },
    {
      "name": "Label (for Svelte)",
      "scope": ["entity.name.label"],
      "settings": {
        "foreground": "#cabf82"
      }
    },
    {
      "name": "Debugger and Console",
      "scope": [
        "keyword.other.debugger",
        "support.class.console",
        "support.function.console"
      ],
      "settings": {
        "foreground": "#f07ad9"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#9a6f5e"
      }
    },
    {
      "name": "Keyword",
      "scope": "punctuation.accessor.optional",
      "settings": {
        "foreground": "#a66a52"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "#797979"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#797979"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#718c8f"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.type.class", "entity.name.class.declaration"],
      "settings": {
        "foreground": "#de8646"
      }
    },
    {
      "name": "Class Usage",
      "scope": ["entity.name.class"],
      "settings": {
        "foreground": "#c9793f"
      }
    },
    {
      "name": "Class usage",
      "scope": ["support.class"],
      "settings": {
        "foreground": "#b6636c"
      }
    },
    {
      "name": "Support",
      "scope": ["support", "support.class.builtin"],
      "settings": {
        "foreground": "#b84f5a"
      }
    },
    {
      "name": "Builtin type",
      "scope": "support.type.builtin",
      "settings": {
        "foreground": "#88b5a7"
      }
    },
    {
      "name": "Primitive type",
      "scope": "support.type.primitive",
      "settings": {
        "foreground": "#847070"
      }
    },
    {
      "name": "Tag",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#c08043"
      }
    },
    {
      "name": "Custom Tag",
      "scope": "support.class.component",
      "settings": {
        "foreground": "#c39635"
      }
    },
    {
      "name": "Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#7a8d9e"
      }
    },
    {
      "name": "Attribute value braces",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#8d7371"
      }
    },
    {
      "name": "Binding pattern",
      "scope": "punctuation.definition.binding-pattern",
      "settings": {
        "foreground": "#6e7b8f"
      }
    },
    {
      "name": "Minor Syntax",
      "scope": [
        "punctuation",
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly",
        "meta.bracket",
        "meta.separator",
        "beginning.punctuation",
        "meta.tag.inline.any",
        "meta.tag.block.any",
        "meta.tag.structure.any",
        "meta.tag.any",
        "meta.tag.sgml.doctype",
        "constant.language.import-export-all",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#797979"
      }
    },
    {
      "name": "Import variable",
      "scope": ["meta.import variable.other.readwrite"],
      "settings": {
        "foreground": "#8d979b"
      }
    },
    {
      "name": "Import alias",
      "scope": ["meta.import variable.other.readwrite.alias"],
      "settings": {
        "foreground": "#c6975c"
      }
    },
    {
      "name": "Deinfitions / Object keys",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name",
        "meta.definition",
        "variable.object.property",
        "constant.language.symbol.hashkey"
      ],
      "settings": {
        "foreground": "#95a49d"
      }
    },
    {
      "name": "Object keys (JSON)",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#afafaf"
      }
    },
    {
      "name": "Svelte assignment",
      "scope": ["source.svelte keyword.operator.assignment"],
      "settings": {
        "foreground": "#2ec029"
      }
    },
    {
      "name": "Svelte prop",
      "scope": ["source.svelte keyword.control.export"],
      "settings": {
        "foreground": "#cabf82"
      }
    },
    {
      "name": "Markdown titles",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#50816a"
      }
    },
    {
      "name": "Markdown italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown code",
      "scope": ["markup.raw", "markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#b8975b"
      }
    },
    {
      "name": "Markdown formatted code",
      "scope": ["meta.embedded"],
      "settings": {
        "foreground": "#afafaf"
      }
    },
    {
      "name": "Markdown link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#577aa0"
      }
    },
    {
      "name": "Types",
      "scope": ["meta.type", "meta.return.type", "keyword.operator.type"],
      "settings": {
        "foreground": "#797979"
      }
    },
    {
      "name": "CSS variables",
      "scope": ["variable.css", "variable.argument.css"],
      "settings": {
        "foreground": "#ae9c8c"
      }
    },
    {
      "name": "CSS id",
      "scope": ["entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#50816a"
      }
    },
    {
      "name": "CSS class",
      "scope": ["entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#577aa0"
      }
    },
    {
      "name": "CSS pseudo class",
      "scope": ["entity.other.attribute-name.pseudo-class.css"],
      "settings": {
        "foreground": "#be777f"
      }
    },
    {
      "name": "CSS pseudo element",
      "scope": ["entity.other.attribute-name.pseudo-element.css"],
      "settings": {
        "foreground": "#be8577"
      }
    },
    {
      "scope": "rainbow1",
      "settings": {
        "foreground": "#998800",
        "fontStyle": ""
      }
    },
    {
      "scope": "keyword.rainbow2",
      "settings": {
        "foreground": "#ae9c8c",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.function.rainbow3",
      "settings": {
        "foreground": "#50816a",
        "fontStyle": ""
      }
    },
    {
      "scope": "comment.rainbow4",
      "settings": {
        "foreground": "#8b84b9",
        "fontStyle": ""
      }
    },
    {
      "scope": "string.rainbow5",
      "settings": {
        "foreground": "#bdb58b",
        "fontStyle": ""
      }
    },
    {
      "scope": "variable.parameter.rainbow6",
      "settings": {
        "foreground": "#993c26",
        "fontStyle": ""
      }
    },
    {
      "scope": "constant.numeric.rainbow7",
      "settings": {
        "foreground": "#ff6ce4",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.type.rainbow8",
      "settings": {
        "foreground": "#c3940a",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.bold.rainbow9",
      "settings": {
        "foreground": "#577aa0",
        "fontStyle": ""
      }
    },
    {
      "scope": "invalid.rainbow10",
      "settings": {
        "foreground": "#afafaf",
        "fontStyle": ""
      }
    },
    {
      "scope": "keyword.control.cucumber.table source",
      "settings": {
        "foreground": "#c6975c"
      }
    },
    {
      "scope": "meta.embedded.inline.snap",
      "settings": {
        "fontStyle": "italic"
      }
    }
  ],

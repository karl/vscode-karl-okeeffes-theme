{
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#888A85"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#EDD400"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#d15537"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#37e5ab"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#d87627"
      }
    },
    {
      "name": "Definition",
      "scope": [
        "meta.definition"
      ],
      "settings": {
        "foreground": "#69a88b"
      }
    },
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#d5c2b1"
      }
    },
    {
      "name": "Template expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#D6D6D6"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#959fa4"
      }
    },
    {
      "name": "Keyword control flow",
      "scope": [
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.trycatch",
        "keyword.control.exception"
      ],
      "settings": {
        "foreground": "#e6dcaa"
      }
    },
    {
      "name": "This",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#b0aada"
      }
    },
    {
      "name": "Debugger and Console",
      "scope": [
        "keyword.other.debugger",
        "support.class.console",
        "support.function.console"
      ],
      "settings": {
        "foreground": "#ffb3ee"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#c7917b"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "#959fa4"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#959fa4"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#69a88b"
      }
    },
    {
      "name": "Tag",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#eeb60f"
      }
    },
    {
      "name": "Custom Tag",
      "scope": "support.class.component",
      "settings": {
        "foreground": "#feab7b"
      }
    },
    {
      "name": "Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#e8c790"
      }
    },
    {
      "name": "Attribute value braces",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#af9896"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#69a88b"
      }
    },
    {
      "name": "Function call",
      "scope": ["meta.function-call", "entity.name.function.tagged-template"],
      "settings": {
        "foreground": "#729FCF"
      }
    },
    {
      "name": "Minor Syntax",
      "scope": [
        "punctuation",
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly",
        "meta.separator",
        "beginning.punctuation",
        "meta.tag.inline.any",
        "meta.tag.block.any",
        "meta.tag.structure.any",
        "meta.tag.any",
        "meta.tag.sgml.doctype",
        "constant.language.import-export-all"
      ],
      "settings": {
        "foreground": "#959fa4"
      }
    },
    {
      "name": "Import variable",
      "scope": [
        "meta.import variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#b1bdc2"
      }
    },
    {
      "name": "Import alias",
      "scope": [
        "meta.import variable.other.readwrite.alias"
      ],
      "settings": {
        "foreground": "#D6D6D6"
      }
    },
    {
      "name": "Object keys (JS/CSS)",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#b8cbc2"
      }
    },
    {
      "name": "Object keys (JSON)",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#D6D6D6"
      }
    },
    {
      "name": "Markdown titles",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#69a88b"
      }
    },
    {
      "name": "Markdown italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown code",
      "scope": ["markup.raw", "markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#e4bc72"
      }
    },
    {
      "name": "Markdown formatted code",
      "scope": ["meta.embedded"],
      "settings": {
        "foreground": "#D6D6D6"
      }
    },
    {
      "name": "Markdown link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#729FCF"
      }
    },
    {
      "name": "Types",
      "scope": [
        "meta.type",
        "meta.return.type",
        "keyword.operator.type"
      ],
      "settings": {
        "foreground": "#959fa4"
      }
    },
    {
      "name": "CSS variables",
      "scope": [
        "variable.css",
        "variable.argument.css"
      ],
      "settings": {
        "foreground": "#69a88b"
      }
    }  ],
  "colors": {
    "statusBar.background": "#363a3c",
    "statusBar.foreground": "#D6D6D6",
    "activityBar.background": "#363a3c",
    "activityBar.foreground": "#D6D6D6",
    "sideBar.background": "#363a3c",
    "sideBar.foreground": "#D6D6D6",
    "sideBarSectionHeader.background": "#363a3c",
    "editor.background": "#3E4345",
    "editor.foreground": "#D6D6D6",
    "editorCursor.foreground": "#D3D7CF",
    "editorLineNumber.foreground": "#4b5153",
    "editorWhitespace.foreground": "#BFBFBF",
    "editor.lineHighlightBackground": "#2326272f",
    "editor.selectionBackground": "#006a7d8c",
    "editor.selectionHighlightBackground": "#59606365",
    "editor.inactiveSelectionBackground": "#60606063",
    "editor.wordHighlightBackground": "#59606356",
    "editor.wordHighlightStrongBackground": "#59606375",
    "editor.findMatchBackground": "#e031314b",
    "editor.findMatchHighlightBackground": "#d46e6e36",
    "editor.findRangeHighlightBackground": "#60606063",
    "editorIndentGuide.background": "#43484a",
    "editorIndentGuide.activeBackground": "#4a5052",
    "scrollbar.shadow": "#0000006b",
    "list.activeSelectionBackground": "#3E4345",
    "list.inactiveSelectionBackground": "#3E4345",
    "list.hoverBackground": "#3a3e40",
    "list.focusBackground": "#484e50",
    // "badge.background": "#fce100",
    // "badge.foreground": "#3E4345",
    "activityBarBadge.background": "#fce100",
    "activityBarBadge.foreground": "#3E4345",
    "dropdown.background": "#363a3d",
    "input.background": "#2f3335",
    "focusBorder": "#cdb700",
    "inputOption.activeBorder": "#69a88b"
  },
  "name": "Karl O'Keeffe's Theme"
}

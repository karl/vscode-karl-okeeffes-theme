{
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#888A85"
      }
    },
    {
      "name": "Unused code",
      "scope": ["comment.unused"],
      "settings": {
        "fontStyle": "",
        "foreground": "#888A85"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#EDD400"
      }
    },
    {
      "name": "Symbol",
      "scope": ["constant.language.symbol"],
      "settings": {
        "foreground": "#b2cf48"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#d15537"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#37e5ab"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other",
        "variable.other.constant.property"
      ],
      "settings": {
        "foreground": "#d87627"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#d5c2b1"
      }
    },
    {
      "name": "Variable definition / Parameter",
      "scope": ["meta.definition variable", "variable.parameter"],
      "settings": {
        "foreground": "#efbc81"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#e5e5e5"
      }
    },
    {
      "name": "Language variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#959fa4"
      }
    },
    {
      "name": "This",
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": {
        "foreground": "#b0aada"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#729FCF"
      }
    },
    {
      "name": "Function",
      "scope": [
        "meta.definition entity.name.function",
        "meta.object-literal.key entity.name.function",
        "meta.function.method entity.name.function"
      ],
      "settings": {
        "foreground": "#4ca1e7"
      }
    },
    {
      "name": "Template expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#D6D6D6"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#959fa4"
      }
    },
    {
      "name": "Keyword control flow",
      "scope": [
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.trycatch",
        "keyword.control.exception"
      ],
      "settings": {
        "foreground": "#f3e69e"
      }
    },
    {
      "name": "Label (for Svelte)",
      "scope": ["entity.name.label"],
      "settings": {
        "foreground": "#f3e69e"
      }
    },
    {
      "name": "Debugger and Console",
      "scope": [
        "keyword.other.debugger",
        "support.class.console",
        "support.function.console"
      ],
      "settings": {
        "foreground": "#ffb3ee"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#c7917b"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "#959fa4"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#959fa4"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#91b2b6"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.type.class"],
      "settings": {
        "foreground": "#feab7b"
      }
    },
    {
      "name": "Class usage",
      "scope": ["support.class"],
      "settings": {
        "foreground": "#ca9196"
      }
    },
    {
      "name": "Support",
      "scope": ["support", "support.class.builtin"],
      "settings": {
        "foreground": "#d57b83"
      }
    },
    {
      "name": "Builtin type",
      "scope": "support.type.builtin",
      "settings": {
        "foreground": "#a7ddcc"
      }
    },
    {
      "name": "Primitive type",
      "scope": "support.type.primitive",
      "settings": {
        "foreground": "#a49595"
      }
    },
    {
      "name": "Tag",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#f1a257"
      }
    },
    {
      "name": "Custom Tag",
      "scope": "support.class.component",
      "settings": {
        "foreground": "#f1ba44"
      }
    },
    {
      "name": "Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#9cb3c7"
      }
    },
    {
      "name": "Attribute value braces",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#af9896"
      }
    },
    {
      "name": "Minor Syntax",
      "scope": [
        "punctuation",
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly",
        "meta.separator",
        "beginning.punctuation",
        "meta.tag.inline.any",
        "meta.tag.block.any",
        "meta.tag.structure.any",
        "meta.tag.any",
        "meta.tag.sgml.doctype",
        "constant.language.import-export-all",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#959fa4"
      }
    },
    {
      "name": "Import variable",
      "scope": ["meta.import variable.other.readwrite"],
      "settings": {
        "foreground": "#b1bdc2"
      }
    },
    {
      "name": "Import alias",
      "scope": ["meta.import variable.other.readwrite.alias"],
      "settings": {
        "foreground": "#efbc81"
      }
    },
    {
      "name": "Deinfitions / Object keys",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name",
        "meta.definition",
        "variable.object.property",
        "constant.language.symbol.hashkey"
      ],
      "settings": {
        "foreground": "#b8cbc2"
      }
    },
    {
      "name": "Object keys (JSON)",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#D6D6D6"
      }
    },
    {
      "name": "Svelte assignment",
      "scope": ["source.svelte keyword.operator.assignment"],
      "settings": {
        "foreground": "#3beb35"
      }
    },
    {
      "name": "Svelte prop",
      "scope": ["source.svelte keyword.control.export"],
      "settings": {
        "foreground": "#f3e69e"
      }
    },
    {
      "name": "Markdown titles",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#69a88b"
      }
    },
    {
      "name": "Markdown italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown code",
      "scope": ["markup.raw", "markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#e4bc72"
      }
    },
    {
      "name": "Markdown formatted code",
      "scope": ["meta.embedded"],
      "settings": {
        "foreground": "#D6D6D6"
      }
    },
    {
      "name": "Markdown link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#729FCF"
      }
    },
    {
      "name": "Types",
      "scope": ["meta.type", "meta.return.type", "keyword.operator.type"],
      "settings": {
        "foreground": "#959fa4"
      }
    },
    {
      "name": "CSS variables",
      "scope": ["variable.css", "variable.argument.css"],
      "settings": {
        "foreground": "#d5c2b1"
      }
    },
    {
      "name": "CSS id",
      "scope": ["entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#69a88b"
      }
    },
    {
      "scope": "rainbow1",
      "settings": {
        "foreground": "#EDD400",
        "fontStyle": ""
      }
    },
    {
      "scope": "keyword.rainbow2",
      "settings": {
        "foreground": "#d5c2b1",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.function.rainbow3",
      "settings": {
        "foreground": "#69a88b",
        "fontStyle": ""
      }
    },
    {
      "scope": "comment.rainbow4",
      "settings": {
        "foreground": "#b0aada",
        "fontStyle": ""
      }
    },
    {
      "scope": "string.rainbow5",
      "settings": {
        "foreground": "#e6dcaa",
        "fontStyle": ""
      }
    },
    {
      "scope": "variable.parameter.rainbow6",
      "settings": {
        "foreground": "#d15537",
        "fontStyle": ""
      }
    },
    {
      "scope": "constant.numeric.rainbow7",
      "settings": {
        "foreground": "#ffb3ee",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.type.rainbow8",
      "settings": {
        "foreground": "#eeb60f",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.bold.rainbow9",
      "settings": {
        "foreground": "#729FCF",
        "fontStyle": ""
      }
    },
    {
      "scope": "invalid.rainbow10",
      "settings": {
        "foreground": "#D6D6D6",
        "fontStyle": ""
      }
    },
    {
      "scope": "keyword.control.cucumber.table source",
      "settings": {
        "foreground": "#efbc81"
      }
    }
  ],
